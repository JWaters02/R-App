convert_monthly_dates <- function(dates_to_convert) {
print(dates_to_convert)
monthly_dates <- c("01-31", "02-28", "03-31", "04-30", "05-31", "06-30", "07-31", "08-31", "09-30", "10-31", "11-30", "12-31")
#monthly_dates <- c("31-", "28-", "31-", "30-", "31-", "30-", "31-", "31-", "30-", "31-", "30-", "31-")
match <- str_match(dates_to_convert, "(\\d+) (\\w+)")
lowercase_month <- str_to_lower(match[,3])
month_index <- monthToNum(lowercase_month)
output <- str_c(match[,2], "-", monthly_dates[month_index])
#output <- str_c(monthly_dates[month_index], lowercase_month, "-", match[,2])
dates <- strptime(output, "%Y-%m-%d", "GMT")
print(dates)
return(dates)
}
convert_monthly_dates("2002 NOV")
